<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="cmen_google_charts.twig_extension" class="CMEN\GoogleChartsBundle\Twig\GoogleChartsExtension" public="false">
            <argument>%cmen_google_charts.version%</argument>
            <argument>%cmen_google_charts.language%</argument>
            <tag name="twig.extension" />
        </service>
    </services>

</container>
